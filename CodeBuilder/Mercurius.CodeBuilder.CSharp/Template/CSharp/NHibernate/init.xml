<?xml version="1.0" encoding="utf-8" ?>
<config>
  <item name="Entity"
        template="NHibernate\Entity.xslt"
        module="interface"
        fileFormat="{0}"
        subFolder="Entities"
        extension="cs"
        handler="Mercurius.CodeBuilder.CSharp.RegisterCreatedFileHandler,Mercurius.CodeBuilder.CSharp"
        project="" />

  <item name="SqlMap"
        dependency="Entity"
