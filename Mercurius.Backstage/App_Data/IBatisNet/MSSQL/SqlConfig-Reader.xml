<?xml version="1.0" encoding="utf-8"?>
<sqlMapConfig xmlns="http://ibatis.apache.org/dataMapper">
  <properties resource="App_Data/IBatisNet/properties.config" />
  <settings>
    <setting validateSqlMap="false" />
    <setting cacheModelsEnabled="true" />
    <setting useStatementNamespaces="true" />
    <setting useReflectionOptimizer="true" />
    <setting useEmbedStatementParams="true" />
  </settings>
  <providers resource="App_Data/IBatisNet/providers.config" />
  <database>
    <provider name="sqlServer2.0" />
    <dataSource name="MSSQLDataSource" connectionString="Data Source=${rhost};Initial Catalog=${rsid};Persist Security Info=True;User ID=${rusername};Password=${rpassword};Min Pool Size=20;" />
  </database>
  <alias>
    <typeAlias alias="DataTable" type="System.Data.DataTable" />
    <typeAlias alias="Params" type="System.Collections.IDictionary" />
    <typeAlias alias="SearchObject" type="Mercurius.Prime.Core.Services.SearchObject, Mercurius.Prime.Core" />
  </alias>
  <sqlMaps>
    <sqlMap embedded="IBatisNet.MSSQL.Dictionary.xml, Mercurius.Core.Implements" />
    <sqlMap embedded="IBatisNet.MSSQL.File.xml, Mercurius.Core.Implements" />
    <sqlMap embedded="IBatisNet.MSSQL.Globalization.xml, Mercurius.Core.Implements" />
    <sqlMap embedded="IBatisNet.MSSQL.Logger.xml, Mercurius.Core.Implements" />
    <sqlMap embedded="IBatisNet.MSSQL.OperationRecord.xml, Mercurius.Core.Implements" />
    <sqlMap embedded="IBatisNet.MSSQL.RepositoryUtils.xml, Mercurius.Core.Implements" />
    <sqlMap embedded="IBatisNet.MSSQL.SystemSetting.xml, Mercurius.Core.Implements" />
    <sqlMap embedded="IBatisNet.MSSQL.User.xml, Mercurius.Core.Implements" />
    <sqlMap embedded="IBatisNet.MSSQL.Permission.xml, Mercurius.Core.Implements" />
    <sqlMap embedded="IBatisNet.MSSQL.HomeShortcut.xml, Mercurius.Core.Implements" />
    <sqlMap embedded="IBatisNet.MSSQL.Organization.xml, Mercurius.Core.Implements" />
  </sqlMaps>
</sqlMapConfig>