<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- 用于IBatis.Net输出日志 -->
    <sectionGroup name="common">
      <section name="logging" type="Common.Logging.ConfigurationSectionHandler, Common.Logging" />
    </sectionGroup>
    <sectionGroup name="iBATIS">
      <section name="logging" type="IBatisNet.Common.Logging.ConfigurationSectionHandler, IBatisNet.Common" />
    </sectionGroup>
  </configSections>

  <common>
    <logging>
      <factoryAdapter type="Common.Logging.Simple.TraceLoggerFactoryAdapter, Common.Logging">
        <!-- 此部分内容在发布时将被Web.Debug.config或Web.Release.config中的相应内容替换-->
        <arg key="level" value="warn" />
        <!--高 off fatal error warn info debug all 低-->
        <arg key="showLogName" value="true" />
        <arg key="showDateTime" value="true" />
        <arg key="dateTimeFormat" value="yyyy-MM-dd HH:mm:ss:fff" />
      </factoryAdapter>
    </logging>
  </common>
  <iBATIS>
    <logging>
      <logFactoryAdapter type="IBatisNet.Common.Logging.Impl.TraceLoggerFA, IBatisNet.Common">
        <!-- 此部分内容在发布时将被Web.Debug.config或Web.Release.config中的相应内容替换-->
        <arg key="level" value="debug" />
        <!--高 off fatal error warn info debug all 低-->
        <arg key="showLogName" value="true" />
        <arg key="showDateTime" value="true" />
        <arg key="dateTimeFormat" value="yyyy-MM-dd HH:mm:ss:fff" />
      </logFactoryAdapter>
    </logging>
  </iBATIS>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="vs:EnableBrowserLink" value="false" />
    <add key="enableSimpleMembership" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />

    <!-- 每页显示记录数 -->
    <add key="DefaultPageSize" value="20" />

    <!-- Redis缓存参数 -->
    <add key="Cache.Redis.Host" value="127.0.0.1" />
    <add key="Cache.Redis.Port" value="6379" />

    <!-- 文件上传token获取参数 -->
    <add key="FileStorage.RomoteUrl" value="http://127.0.0.1:8000/FileStorage/" />
    <add key="FileStorage.Token.Account" value="dezo" />
    <add key="FileStorage.Token.Password" value="dezo@123" />
    <add key="FileStorage.Token.TokenEndpointPath" value="api/token" />
    <add key="FileStorage.UploadUrl" value="api/FileStorage/Upload" />
    <add key="FileStorage.UploadWithBase64Url" value="api/FileStorage/Upload/Base64" />
    <add key="FileStorage.Remove" value="api/FileStorage/Remove" />
  </appSettings>

  <system.data>
    <DbProviderFactories>
      <add name="Npgsql Data Provider" invariant="Npgsql" description="posteSQL provider" type="Npgsql.NpgsqlFactory, Npgsql, Version=3.0.2.0, Culture=neutral, PublicKeyToken=5d8b90d52f46fda7" />
    </DbProviderFactories>
  </system.data>

  <location path="~/favicon.ico">
    <system.web>
      <authorization>
        <allow users="*" />
      </authorization>
    </system.web>
  </location>

  <!-- 忽略针对图表的用户验证 -->
  <location path="asset.axd">
    <system.web>
      <authorization>
        <allow users="*" />
      </authorization>
    </system.web>
  </location>

  <!-- 忽略对js、css脚本压缩的用户验证 -->
  <location path="bundles">
    <system.web>
      <authorization>
        <allow users="*" />
      </authorization>
    </system.web>
  </location>

  <!-- 忽略对用户登录的验证 -->
  <location path="Account">
    <system.web>
      <authorization>
        <allow users="*" />
      </authorization>
    </system.web>
  </location>

  <location path="Api">
    <system.web>
      <authorization>
        <allow users="*" />
      </authorization>
    </system.web>
  </location>

  <system.web>
    <compilation debug="true" defaultLanguage="c#" targetFramework="4.6" />
    <httpRuntime maxRequestLength="204800" requestValidationMode="2.0" targetFramework="4.5" />

    <!-- 所有请求都必须进行验证 -->
    <authorization>
      <deny users="?" />
    </authorization>
    <authentication mode="Forms">
      <forms name="Mercurius.BackstageAuth" loginUrl="~/Account/LogOn" timeout="60" cookieless="UseCookies" enableCrossAppRedirects="true" />
    </authentication>
    <!--<sessionState mode="InProc" cookieless="UseCookies" cookieName="s2id" timeout="60" />-->

    <!-- 基于Redis的共享Session -->
    <sessionState mode="Custom" customProvider="RedisSessionStateStore" cookieless="UseCookies" cookieName="s2id" timeout="60">
      <providers>
        <clear/>
        <add name="RedisSessionStateStore"
             type="Microsoft.Web.Redis.RedisSessionStateProvider"
             host="127.0.0.1"
             port="6379"
             ssl="false" />
      </providers>
    </sessionState>
    
    <!-- cookies设置 -->
    <httpCookies domain="" httpOnlyCookies="true" />

    <!-- Asp.Net加密密钥 -->
    <machineKey validationKey="10C1123E650930E91AC0D87A46E11CF2C41577CB9397494AFCB851099E2F7C55C1A336B647A42B00D0BA41135FF00E903D29801227CD4628CC62CE71DFD31AFE" decryptionKey="793AADF3F4D646ADAB4C66BA9E94366EC5593571D6CE05DA" validation="SHA1" decryption="AES" />
  </system.web>

  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true">
    </modules>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
    <staticContent>
      <remove fileExtension=".woff" />
      <remove fileExtension=".woff2" />
      <mimeMap fileExtension=".woff" mimeType="application/x-font-woff" />
      <mimeMap fileExtension=".woff2" mimeType="application/x-font-woff" />
    </staticContent>
  </system.webServer>
