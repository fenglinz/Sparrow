@{
    var unAllotUsers = this.ViewBag.UnAllotUsers as ResponseSet<User>;
}
<table id="table1" class="grid">
    <thead>
        <tr>
            <td style="width: 40px;">
                <label class="check-all" title="全选">&nbsp;</label>
            </td>
            <td style="text-align: center;">账号</td>
        </tr>
    </thead>
    <tbody>
        @if (unAllotUsers.HasData())
            {
                foreach (var user in unAllotUsers.Datas)
                {
                <tr>
                    <td style="text-align:center;">
                        <input type="checkbox" value="@user.Id" />
                    </td>
                    <td style="border-right: 0; text-align: center;">@user.Account</td>
                </tr>
            }
        }
        else if (unAllotUsers.HasError())
        {
            <tr>
                <td colspan="4" class="text-danger">@unAllotUsers.GetErrorMessage()</td>
            </tr>
        }
        else
        {
            <tr>
                <td colspan="4" class="empty-data">@Html.GetGlobalValue("empty-data")</td>
            </tr>
        }
    </tbody>
</table>